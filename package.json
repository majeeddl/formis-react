{
  "name": "formis-react",
  "version": "1.0.7",
  "description": "This is a react form generator",
  "license": "MIT",
  "author": "Majeed Dourandeesh",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/majeeddl/formis-react"
  },
  "keywords": [
    "form",
    "generator",
    "json"
  ],
  "files": [
    "dist",
    "README.md"
